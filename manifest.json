{
  "name": "Salesforce Enhancement Suite",
  "version": "0.0.1",  
  "description": "Collection of Salesforce enhancements",
  "manifest_version": 2,
  "permissions": [ "tabs", "https://*.force.com/*", "https://*.salesforce.com/*"],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "rbirbeck",
  "content_scripts": [ 
    // {
    //  "js": [  "js/jquery.js",
    //           "js/contentscript.js" ],
    //  "matches": [ "https://*.salesforce.com/setup/*", "https://*.force.com/setup/*" ]
    // },
    {
      "js": [ "js/jquery.js",
              "js/morerecords.js"],
      "matches": ["https://*.salesforce.com/*"]
    },
    {
      "js": [ "js/jquery.js",
              "js/biggerWorkbenchExecuteBox.js"],
      "matches": ["http://workbench/execute.php", "https://workbench.developerforce.com/execute.php", "https://workbench.internal.salesforce.com/execute.php"]
    },
    {
      "css": ["css/ses.css"],
      "js": [ "js/jquery.js",
              "js/prettyLog.js"],
      "matches": ["https://*.salesforce.com/apexdebug/traceDownload*"]
    }
  ]
}
